{
  "name": "qi-antd",
  "version": "0.1.0",
  "private": true,
  "description": "React UI Components",
  "main": "build/index.js",
  "module": "build/index.js",
  "types": "build/index.d.js",
  "author": "smartzhao",
  "license": "MIT",
  "keywords": [
    "React",
    "UI",
    "Component",
    "Components"
  ],
  "homepage": "https://github.com/varfate/qi-antd",
  "repository": {
    "type": "git",
    "url": "https://github.com/varfate/qi-antd"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.3.0-beta3",
    "@fortawesome/free-regular-svg-icons": "^6.0.0-beta3",
    "@fortawesome/free-solid-svg-icons": "^6.0.0-beta3",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "axios": "^0.25.0",
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-transition-group": "^4.4.2",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "npm run storybook",
    "build:ts": "tsc --project tsconfig.build.json",
    "build:css": "tsc --project tsconfig.production.json --clean ",
    "test": "react-scripts test",
    "storybook": "start-storybook -p 6006 -s public",
    "build:storybook": "build-storybook -o docs"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/addon-viewport": "^6.4.15",
    "@storybook/builder-webpack5": "^6.4.14",
    "@storybook/manager-webpack5": "^6.4.14",
    "@storybook/node-logger": "^6.4.14",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.14",
    "@types/react-transition-group": "^4.4.4",
    "sass": "^1.49.0",
    "webpack": "^5.67.0"
  }
}
